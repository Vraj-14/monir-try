                        android:layout_width="129dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0"
                        android:paddingLeft="40dp"
                        android:paddingTop="70dp"
                        android:scaleType="center"
                        app:srcCompat="@drawable/group_4" />

                    <ImageView
